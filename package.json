{
  "name": "humanjs",
  "description": "CLI tool for generating single page apps a. la. http://humanjavascript.com",
  "version": "0.2.0",
  "preferGlobal": true,
  "dependencies": {
    "commander": "1.1.1",
    "colors": "0.6.0-1",
    "ncp": "0.4.2",
    "readable-stream": "1.0.2",
    "underscore": "1.5.2",
    "jade": "~1.0.2",
    "slugger": "0.0.3"
  },
  "devDependencies": {
    "rimraf": "*"
  },
  "repository": {
    "type": "git",
    "url": "HenrikJoreteg/humanjs"
  },
  "scripts": {
    "buildDocs": "node ./scripts/buildDocs.js",
    "publishDocs": "./scripts/publishDocs.sh",
    "start": "node ./bin/humanjs"
  },
  "bin": {
    "humanjs": "./bin/humanjs"
  }
}
